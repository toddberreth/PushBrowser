<SETTINGS>
	<WINDOW> 
		<WIDTH>1280</WIDTH>
		<HEIGHT>720</HEIGHT>
		<FULLSCREEN>TRUE</FULLSCREEN>
		<HIDE_CURSOR>FALSE</HIDE_CURSOR>
	</WINDOW>
	<DEFAULT_URL>http://www.google.com</DEFAULT_URL>
	<QR_CONTROL>
		<QR_ON>TRUE</QR_ON>
		<DEFAULT_QR_URL>http://www.google.com</DEFAULT_QR_URL>
		<QR_SIDE>LEFT</QR_SIDE> 	<!--  LEFT, RIGHT, BOTH -->
		<QR_HEIGHT>BOTTOM</QR_HEIGHT> 	<!--  TOP, MIDDLE, BOTTOM -->
		<QR_SIZE>128</QR_SIZE>		<!-- in pixels -->
		<QR_MARGIN>32</QR_MARGIN>	<!-- between side of screen and qr code, in pixels -->
	<QR_CONTROL>
    	<OSC_RECEIVE_PORT>57120</OSC_RECEIVE_PORT>
</SETTINGS>
